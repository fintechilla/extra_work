package com.luv2code.springdemo;

public class SwimCoach implements Coach {
	
	private HappyFortuneService happyFortuneService;
	
	public SwimCoach(HappyFortuneService happyFortuneService) {
		this.happyFortuneService = happyFortuneService;
	}

	@Override
	public String getDailyWorkout() {
		return "Swim hard 50 lapses";
	}

	@Override
	public String getFortune() {
		return happyFortuneService.getFortuneService();
	}

}
